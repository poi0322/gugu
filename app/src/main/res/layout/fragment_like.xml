<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
    android:id="@+id/text_like"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginTop="20dp"
    android:layout_marginEnd="8dp"
        android:text="관심목록"
    android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@android:color/black"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_like_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="관심 글"
        android:textSize="15sp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="@id/text_like"
        app:layout_constraintEnd_toStartOf="@id/text_like"
        app:layout_constraintTop_toBottomOf="@id/text_like"
        />

    <ListView
        android:id="@+id/like_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="56dp"
        app:layout_constraintTop_toBottomOf="@+id/text_like_list" />


</androidx.constraintlayout.widget.ConstraintLayout>